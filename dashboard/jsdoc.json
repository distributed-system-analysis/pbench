{
    "source": {
        "include":["src"],
        "includePattern":".js$",
        "excludePattern":"(node_modules/|docs)"
    },
    "plugins":["plugins/markdown"],
    "templates":{
        "cleverLinks":true,
        "monospaceLinks":true
    },
    "opts":{
        "recurse":true,
        "destination":"./docs/"
    }
}
