{
    "pbench-run": {
        "_all" : {"enabled" : false},
        "_meta": {
            "version": "3"
        },
        "date_detection": false,
        "properties": {
            "@timestamp": {
                "type": "date"
            },
            "@generated-by": {
                "type": "string",
                "index": "not_analyzed"
            },
            "@metadata": {
                "properties": {
                    "controller_dir": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "file-date": {
                        "type": "date"
                    },
                    "file-name": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "file-size": {
                        "type": "long"
                    },
                    "md5": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "pbench-agent-version": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "result-prefix": {
                        "type": "string"
                    },
                    "satellite": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "toc-prefix": {
                        "type": "string"
                    }
                }
            },
            "host_tools_info": {
                "type": "nested",
                "properties": {
                    "hostname": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "hostname-f": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "hostname-s": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "label": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "tools": {
                        "properties": {
                            "disk": {
                                "type": "string"
                            },
                            "haproxy-ocp": {
                                "type": "string"
                            },
                            "iostat": {
                                "type": "string"
                            },
                            "mpstat": {
                                "type": "string"
                            },
                            "oc": {
                                "type": "string"
                            },
                            "perf": {
                                "type": "string"
                            },
                            "pidstat": {
                                "type": "string"
                            },
                            "pprof": {
                                "type": "string"
                            },
                            "proc-interrupts": {
                                "type": "string"
                            },
                            "proc-vmstat": {
                                "type": "string"
                            },
                            "prometheus-metrics": {
                                "type": "string"
                            },
                            "sar": {
                                "type": "string"
                            },
                            "turbostat": {
                                "type": "string"
                            },
                            "vmstat": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "run": {
                "properties": {
                    "id": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "controller": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "user": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "name": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "script": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "config": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "date": {
                        "type": "date"
                    },
                    "start": {
                        "type": "date"
                    },
                    "end": {
                        "type": "date"
                    },
                    "toolsgroup": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "iterations": {
                        "type": "string"
                    }
                }
            },
            "sosreports": {
                "type": "nested",
                "properties": {
                    "hostname-f": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "hostname-s": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "md5": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "name": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "inet": {
                        "type": "nested",
                        "properties": {
                            "ifname": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "ipaddr": {
                                "type": "ip"
                            }
                        }
                    },
                    "inet6": {
                        "type": "nested",
                        "properties": {
                            "ifname": {
                                "type": "string",
                                "index": "not_analyzed"
                            },
                            "ipaddr": {
                                "type": "string",
                                "index": "not_analyzed"
                            }
                        }
                    }
                }
            }
        }
    }
}
