{
    "pbench-server-reports": {
        "_all" : {"enabled" : false},
        "_meta": {
            "version": "2"
        },
        "date_detection": false,
        "properties": {
            "@timestamp": {
                "type": "date"
            },
            "chunk_id": {
                "type": "integer"
            },
            "doctype": {
                "type": "string",
                "index": "not_analyzed"
            },
            "name": {
                "type": "string",
                "index": "not_analyzed"
            },
            "text": {
                "type": "string"
            },
            "total_chunks": {
                "type": "integer"
            },
            "total_size": {
                "type": "integer"
            }
        }
    }
}
