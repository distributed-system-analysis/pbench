{
    "pbench-run": {
        "_timestamp": {
            "enabled": true,
            "store": true,
            "fielddata": {
                "format": "doc_values"
            }
        },
        "properties": {
            "_metadata": {
                "properties": {
                    "generated-by-version": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "file-name": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "file-date": {
                        "format": "dateOptionalTime",
                        "type": "date"
                    },
                    "md5": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "generated-by": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            },
            "tools": {
                "type": "nested",
                "properties": {
                    "host": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            },
            "pbench": {
                "properties": {
                    "pbench-rpm-version": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            },
            "table-of-contents": {
                "type": "object"
            },
            "run": {
                "properties": {
                    "start_run": {
                        "type": "string",
                        "index": "not_analyzed"
                        
                    },
                    "name": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "config": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "controller": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "script": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "date": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "end_run": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            }
        }
    }
}
