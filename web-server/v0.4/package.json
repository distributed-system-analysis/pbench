{
  "name": "pbench_dashboard",
  "version": "2.0.0",
  "description": "UI solution for scalable visualization of benchmark data.",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "site": "umi-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "umi test ./src/components ./src/pages --verbose --maxWorkers=1 --runInBand",
    "test:component": "umi test @",
    "test:e2e": "umi test ./src/e2e --verbose",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@antv/g2": "^3.4.10",
    "@babel/runtime": "^7.3.1",
    "@nivo/bar": "^0.36.0",
    "ant-design-pro": "^2.1.1",
    "antd": "^3.16.1",
    "classnames": "^2.2.5",
    "dva": "^2.4.1",
    "dva-core": "^1.1.0",
    "enquire-js": "^0.2.1",
    "html2canvas": "^1.0.0-alpha.12",
    "jschart": "^1.0.7",
    "jspdf": "^1.5.3",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.1.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "rc-drawer": "^1.2.0",
    "react": "^16.4.1",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.3",
    "react-fittext": "^1.0.0",
    "redux": "3.x",
    "redux-persist": "^5.10.0",
    "setprototypeof": "^1.1.0",
    "umi": "^2.6.16",
    "umi-request": "^1.0.7",
    "url-polyfill": "^1.0.10"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.1.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^1.3.1",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "lint-staged": "^7.2.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "1.14.0",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.5.0",
    "regenerator-runtime": "^0.12.0",
    "stylelint": "^9.2.1",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-config-standard": "^18.0.0",
    "umi-plugin-react": "^1.7.5"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "optionalDependencies": {
    "puppeteer": "^1.4.0"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
